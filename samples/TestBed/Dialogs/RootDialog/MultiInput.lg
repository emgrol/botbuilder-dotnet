# ButtonForIntent
[Activity
    Attachments = ${json(fromFile('./ButtonToRoute_ValueRecognizer.json'))}
]

# MultiProperty
[Activity
    Attachments = ${json(fromFile('./MultiInput.json'))}
]

# MultiProperty.invalid(type)
[Activity
    Text = Sorry, **${this.value}** does not work. I'm looking for a ${invalidTypeBasedHelp(type)}.
    ${MultiProperty()}
]

# invalidTypeBasedHelp(type)
- Switch : ${type}
    - CASE : ${'email'}
        - valid email address.. e.g. info@contoso.com
    - CASE : ${'age'}
        - a number between 1 - 150
    - DEFAULT :
        - valid